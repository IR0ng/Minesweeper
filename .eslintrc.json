{
  "extends": [
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "rules": {
  
    "prettier/prettier": [
      "warn",
      
      {
        "endOfLine": "auto"
      }
    ]
  }
}